{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include":["js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/", 
        "recurse": true
    }
}